[{"app_id": "freeciv-gtk2.desktop", "date_created": 1539478125.0, "description": "I first found this game 6-7 years ago during a mental hospital stay, where we had an hour to use desktops the hospital had for recreation. For obvious reasons, there was no internet on these machines, but they did have games - including FreeCiv.\n\nWhile I was terrible at the game and only started getting good at it very recently, it was an amazing escape from a sometimes stressful time. The fact that it is open-source makes the experience even better.", "distro": "Ubuntu", "karma_down": 1, "karma_up": 22, "locale": "en_US", "rating": 100, "reported": 0, "review_id": 9780, "score": 19, "summary": "Amazing Open-Source Game", "user_display": "Alexis", "version": "2.5.10-1"}, {"app_id": "freeciv-gtk2.desktop", "date_created": 1489815560.0, "description": "Wonderful graphics and all.", "distro": "Fedora", "karma_down": 14, "karma_up": 10, "locale": "en_US", "rating": 100, "reported": 0, "review_id": 2508, "score": 0, "summary": "Great game", "user_display": "Michael D. Beard", "version": "2.5.6"}, {"app_id": "freeciv-gtk2.desktop", "date_created": 1490658117.0, "description": "Runs well with plenty of game play options to keep your imagination active, would definitely recommend to anybody who loves these kind of games.", "distro": "Fedora", "karma_down": 0, "karma_up": 5, "locale": "en_US", "rating": 100, "reported": 0, "review_id": 2667, "score": 0, "summary": "Truly a fun classic", "user_display": "Cisco", "version": "2.5.6"}, {"app_id": "freeciv-gtk2.desktop", "date_created": 1518945776.0, "description": "All I know of politics I learned from Freeciv.", "distro": "Fedora", "karma_down": 0, "karma_up": 9, "locale": "en_DK", "rating": 100, "reported": 0, "review_id": 6486, "score": 0, "summary": "Classic", "user_display": "Juha Siltala", "version": "2.5.10"}, {"app_id": "freeciv-gtk2.desktop", "date_created": 1560770007.0, "description": "Open source solid 4X strategy game, feels like a g