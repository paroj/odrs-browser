[{"app_id": "Ri-li.desktop", "date_created": 1499666704.0, "description": "Looking at the screenshots and navigating the initial menus, I expected this to be a real disaster. Instead, it turned out to be a kid-friendly snake-type game. You navigate a wooden train through maze-like tracks in order to collect additional cars. As your train gets longer, you have to take more and more care not to run into yourself as you snake around the track, looking for the last few cars.\n\nI don't know that this will challenge adults, but I can imagine that kids will enjoy the simple, undemanding gameplay and the toy-themed visuals.", "distro": "Fedora", "karma_down": 5, "karma_up": 23, "locale": "en_US", "rating": 80, "reported": 1, "review_id": 3808, "score": 0, "summary": "Better than expected; a Snake game", "user_display": "micah", "version": "2.0.1"}, {"app_id": "Ri-li.desktop", "date_created": 1568129474.0, "description": "Simplistic fun idea, very well executed.", "distro": "Ubuntu", "karma_down": 0, "karma_up": 0, "locale": "en_US", "rating": 100, "reported": 0, "review_id": 82579, "score": 0, "summary": "Simplistic Genius", "user_display": "( \u0361~ \u035c\u0296 \u0361\u00b0)", "version": "2.0.1"}, {"app_id": "Ri-li.desktop", "date_created": 1577097402.0, "description": "it is useful for kids like 1 to 4", "distro": "Ubuntu", "karma_down": 0, "karma_up": 0, "locale": "en_IN", "rating": 100, "reported": 0, "review_id": 127365, "score": 0, "summary": "its concentration game", "user_display": "shankar", "version": "2.0.1"}, {"app_id": "Ri-li.desktop", "date_created": 1577473034.0, "description": "The controls are easy to understand and use.", "distro": "Ubuntu", "karma_down": 0, "karma_up": 0, "locale": "en_CA", "rating": 100, "reported": 0, "review_id": 129147, "score": 0, "summary": "Very playable", "user_display": "ubuntu", "version": "2.0.1"}, {"app_id": "Ri-li.desktop", "date_created": 1586658919.0, "description": "Don't waste your time, no control, crashed and locked 