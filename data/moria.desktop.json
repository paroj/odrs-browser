[{"app_id": "moria.desktop", "date_created": 1561318598.0, "description": "How am I supposed to play the game? I can't understand it at all", "distro": "Ubuntu", "karma_down": 1, "karma_up": 0, "locale": "en_GB", "rating": 20, "reported": 0, "review_id": 51288, "score": 0, "summary": "How do I play this?", "user_display": "Fl\u00e1vio", "version": "5.6.debian.1"}, {"app_id": "moria.desktop", "date_created": 1571619366.0, "description": "I used to play this on my old ibm 8086 waaaay back in the day. I prefer this game over rogue, you can go to town and haggle with shop keepers.\nUse the numbers on the right side of your keyboard for controls. (2 is down, 8 is up, etc..)", "distro": "Ubuntu", "karma_down": 0, "karma_up": 0, "locale": "en_CA", "rating": 100, "reported": 0, "review_id": 98937, "score": 0, "summary": "Classic Moria", "user_display": "xman", "version": "5.7.10"}, {"app_id": "moria.desktop", "date_created": 1572446353.0, "description": "The game in itself seems very fun, but there should really be a menu beside showing where each button goes (up down left right diagonal) otherwise it's almost impossible to play", "distro": "Ubuntu", "karma_down": 0, "karma_up": 0, "locale": "en_CA", "rating": 60, "reported": 0, "review_id": 103536, "score": 0, "summary": "nice concept, hard to grasp", "user_display": "Yavik", "version": "5.7.10"}, {"app_id": "moria.desktop", "date_created": 1591364970.0, "description": "...you should make controls easier.", "distro": "Ubuntu", "karma_down": 0, "karma_up": 0, "locale": "en_US.UTF-8", "rating": 100, "reported": 0, "review_id": 224538, "score": 0, "summary": "Great App but..", "user_display": "Mehmet Akif", "version": "5.7.10"}, {"app_id": "moria.desktop", "date_created": 1601188506.0, "description": "Nah just joking, this is way harder! Just expect to die and you'll have a lot more fun.", "distro": "Ubuntu", "karma_down": 0, "karma_up": 0, "locale": "en_AU.UTF-8", "rating": 100, "reported": 0, "review_id": 276249, "score": 0