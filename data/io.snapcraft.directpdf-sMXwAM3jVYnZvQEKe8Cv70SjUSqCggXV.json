[{"app_id": "io.snapcraft.directpdf-sMXwAM3jVYnZvQEKe8Cv70SjUSqCggXV", "date_created": 1585585473.0, "description": "Downloaded but could not get it to run. got the following message.\nNo parameters defined. Using current working directory.\nterminate called after throwing an instance of 'std::invalid_argument'\n what(): File does not exist.\nAborted (core dumped)", "distro": "Ubuntu", "karma_down": 0, "karma_up": 0, "locale": "en_US", "rating": 20, "reported": 0, "review_id": 173449, "score": 0, "summary": "Don't Bother", "user_display": "Mark", "version": "0.8.3"}, {"app_id": "io.snapcraft.directpdf-sMXwAM3jVYnZvQEKe8Cv70SjUSqCggXV", "date_created": 1586002189.0, "description": "It works fine. Work with acceptable accuracy. Tested on English language only. There is no user interface. You can't run it by clicking Lunch. Need to run the following command --\n \nConvert all images in given directory:\ndirectpdf -i /home/user/images -o output.pdf\n \nConvert image to PDF with title:\ndirectpdf -i /home/user/image.png -o output.pdf -t Title\n \nConvert images in given directory and protect output PDF file with passwords and apply AESv3 encryption:\n directpdf -i /home/user/images -o output.pdf -p openPassword -m masterPassword -a AESv3\n \nConvert images in given directory, protect output PDF file with master password, apply RC4v2_128 encryption and apply some permissions for document:\ndirectpdf -i /home/user/images -o output.pdf -m ownerPassword -a RC4v2_128 -e print,edit,copy", "distro": "Ubuntu", "karma_down": 0, "karma_up": 0, "locale": "en_US", "rating": 100, "reported": 0, "review_id": 176250, "score": 0, "summary": "It works grate", "user_display": "somenomus", "version": "0.8.3"}, {"app_id": "io.snapcraft.directpdf-sMXwAM3jVYnZvQEKe8Cv70SjUSqCggXV", "date_created": 1586252399.0, "description": "Simple and usefull", "distro": "Ubuntu", "karma_down": 0, "karma_up": 0, "locale": "en_IL", "rating": 100, "reported": 0, "review_id": 178125, "score": 0, "summary": "S