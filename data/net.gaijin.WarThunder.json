[{"app_id": "net.gaijin.WarThunder", "date_created": 1722015690.0, "description": "Great game, over 4k hours and more to come. Been playing this game since 2017, and I really like it. \nHowever, this version does not really work that great, so I moved over to launching the Linux-version from Steam instead, works much better there", "distro": "openSUSE Tumbleweed", "karma_down": 0, "karma_up": 0, "locale": "en_US", "rating": 80, "reported": 0, "review_id": 628464, "score": 31, "summary": "Good, but prefer to launch via Steam", "user_display": "Jonathan S\u00e9lea", "version": "1.0.3.3"}, {"app_id": "net.gaijin.WarThunder", "date_created": 1718767204.0, "description": "Crashes on launch with a bash error on Fedora 40", "distro": "Ultramarine Linux", "karma_down": 0, "karma_up": 0, "locale": "en_AU", "rating": 20, "reported": 0, "review_id": 625663, "score": 23, "summary": "Nope", "user_display": "Chris", "version": "1.0.3.3"}, {"app_id": "net.gaijin.WarThunder", "date_created": 1654742989.0, "description": "This game looks great from the screenshots, but it took over an hour to download files for the game. When I launched the game, the screen was filled with green pixels. You could barely make out any button to press on the game. I could barely see any shapes on the screen, and looking at them hurt my eyes. I think this game would be really fun and functional if the graphics worked.", "distro": "Fedora", "karma_down": 12, "karma_up": 37, "locale": "en_US.UTF-8", "rating": 40, "reported": 0, "review_id": 493754, "score": 21, "summary": "Looks good, but may not function", "user_display": "Kazadi Family", "version": "0.9.4.83"}, {"app_id": "net.gaijin.WarThunder", "date_created": 1716857005.0, "description": "Totally borked. Doesn't launch. Oh well.", "distro": "Fedora Linux", "karma_down": 0, "karma_up": 0, "locale": "en_US.UTF-8", "rating": 20, "reported": 0, "review_id": 623553, "score": 14, "summary": "Doesn't work", "user_display": "Ivan Jarmoluk", "version": "0.9.4