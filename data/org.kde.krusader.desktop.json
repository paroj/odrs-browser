[{"app_id": "org.kde.krusader.desktop", "date_created": 1718282838.0, "description": "This is an essential app if you have large numbers of user generate files on your system. Simplistic drag and drop apps just plain fail when doing complex operations. While Krusader supports drag and drop, I've never been a fan. I want to quickly select files and do something very concrete with them. Move, copy, delete, etc. I use file extensions, most file managers today are at best awkward in their support of extensions. Krusader has excellent support for extensions. It also has good SCP support allowing you to keep multiple machines synched up. Krusader has a number of features most file managers today lack. It is far more than a 2 pane manager. I keep dozens of tabs open between the 2 panes allowing me to do very complex sorting with ease. Even when I am dealing with thousands of files.", "distro": "Ubuntu", "karma_down": 0, "karma_up": 0, "locale": "en_US", "rating": 100, "reported": 0, "review_id": 625181, "score": 33, "summary": "Awesome and very configurable file manager", "user_display": "draciron", "version": "2.7.2-2"}, {"app_id": "org.kde.krusader.desktop", "date_created": 1718876778.0, "description": "great tool but currently has a problem with KF6 integration", "distro": "KDE neon", "karma_down": 0, "karma_up": 0, "locale": "en_US", "rating": 80, "reported": 0, "review_id": 625758, "score": 23, "summary": "good, but KDE6 issues", "user_display": "Denis", "version": "2.8.1-0xneon"}, {"app_id": "org.kde.krusader.desktop", "date_created": 1513178351.0, "description": "I use this application for several years and I am very thankful to developers!", "distro": "Ubuntu", "karma_down": 0, "karma_up": 7, "locale": "en_US", "rating": 100, "reported": 0, "review_id": 5729, "score": 0, "summary": "The greatest twin panel file manager!", "user_display": "Gilmanov Ildar", "version": "2.6.0-1"}, {"app_id": "org.kde.krusader.desktop", "date_created": 1546390340.0, "description": "Hav