[{"app_id": "org.flarerpg.Flare.desktop", "date_created": 1528495749.0, "description": "A single run takes about 5 to 6 hours.", "distro": "Fedora", "karma_down": 0, "karma_up": 1, "locale": "en_GB", "rating": 100, "reported": 0, "review_id": 8069, "score": 0, "summary": "Great Dungeon Crawler", "user_display": "fb", "version": "1.0"}, {"app_id": "org.flarerpg.Flare", "date_created": 1543855213.0, "description": "Flare's \"Empyrean\" storyline basically involves the main character releasing himself from exile by finding the three components of gravebloom ink. To do that you have to advance through several dungeons. Progression is very linear and the difficulty curve is all over the place. Would recommend another RPG instead, like Morrowind or Freedroid.", "distro": "Debian GNU/Linux", "karma_down": 1, "karma_up": 0, "locale": "en_US", "rating": 60, "reported": 0, "review_id": 11382, "score": 0, "summary": "Too linear", "user_display": "Ivan Stanton", "version": "1.0"}, {"app_id": "org.flarerpg.Flare", "date_created": 1605504326.0, "description": "Finding out where to go next is always the question. Very enjoyable game.", "distro": "Ubuntu", "karma_down": 0, "karma_up": 0, "locale": "en_US.UTF-8", "rating": 100, "reported": 0, "review_id": 303973, "score": 0, "summary": "Great game", "user_display": "Ellen", "version": "1.0"}, {"app_id": "org.flarerpg.Flare", "date_created": 1618998848.0, "description": "Very nice...", "distro": "Arch Linux", "karma_down": 0, "karma_up": 0, "locale": "en_US.UTF-8", "rating": 100, "reported": 0, "review_id": 367396, "score": 0, "summary": "Nice", "user_display": "Solidsnake", "version": "1.11"}, {"app_id": "org.flarerpg.Flare", "date_created": 1672663861.0, "description": "If you're looking for a decent ARPG game that is FOSS. This one is for you", "distro": "Ubuntu", "karma_down": 0, "karma_up": 0, "locale": "en_PH", "rating": 100, "reported": 0, "review_id": 552365, "score": 0, "summary": "Good ARPG game", "user_display": "Richard O