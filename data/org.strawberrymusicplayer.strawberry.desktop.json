[{"app_id": "org.strawberrymusicplayer.strawberry", "date_created": 1613351226.0, "description": "Good to see someone keeping Clementine up to date!\n\nUnlike Clementine, Strawberry displays well on a HiDPI screen on KDE Plasma. Also like the Context and Collection organsiation as better.", "distro": "openSUSE Leap", "karma_down": 0, "karma_up": 0, "locale": "en_GB", "rating": 100, "reported": 0, "review_id": 341069, "score": 0, "summary": "Excellent up to date version of Clementine", "user_display": "Paul", "version": "0.8.5-1.lp152"}, {"app_id": "org.strawberrymusicplayer.strawberry", "date_created": 1636386411.0, "description": "I only have around 13,000 songs but does a great job of organizing them all! I especially like the ability to autofind lyrics.", "distro": "Ubuntu", "karma_down": 0, "karma_up": 0, "locale": "en_CA.UTF-8", "rating": 100, "reported": 0, "review_id": 436246, "score": 0, "summary": "Love it!", "user_display": "Plum", "version": "1.0.0"}, {"app_id": "org.strawberrymusicplayer.strawberry", "date_created": 1644978936.0, "description": "Unable to find second hard drive or network drive. That is where all my music resides. This looks like a great program, but without that capability it's no good to me. Sorry.", "distro": "Ubuntu", "karma_down": 0, "karma_up": 0, "locale": "en_US.UTF-8", "rating": 40, "reported": 0, "review_id": 463575, "score": 0, "summary": "Finding network drives should work", "user_display": "Pete", "version": "1.0.0"}, {"app_id": "org.strawberrymusicplayer.strawberry", "date_created": 1654796740.0, "description": "Best music manager for linux. I think that visuals could be improve but it also allows for interface personalization so not a big problem. Overall a great great app", "distro": "Fedora Linux", "karma_down": 0, "karma_up": 0, "locale": "en_US.UTF-8", "rating": 100, "reported": 0, "review_id": 494033, "score": 0, "summary": "Best music manager for linux", "user_display": "Javier Francisco Castilla Jim\u00e9nez", "version": "1.0.4"}, {"app_id": "org.strawberrymusicplayer.strawberry", "date_created": 1660530020.0, "description": "Does everything I need it to do.", "distro": "Fedora Linux", "karma_down": 0, "karma_up": 0, "locale": "en_US.UTF-8", "rating": 100, "reported": 0, "review_id": 509872, "score": 0, "summary": "As good as Clemetine was", "user_display": "Gone Postal", "version": "1.0.6"}, {"app_id": "org.strawberrymusicplayer.strawberry.desktop", "date_created": 1611829141.0, "description": "Definitely the best music player is Strawberry v. 0.6.7. No bugs, searches and displays song covers and lyrics, supports many themes, a great graphical equalizer as well as a user friendly interface.", "distro": "Ubuntu", "karma_down": 0, "karma_up": 0, "locale": "en_US", "rating": 100, "reported": 0, "review_id": 333746, "score": 0, "summary": "Excellent !", "user_display": "kalea", "version": "0.6.7"}, {"app_id": "org.strawberrymusicplayer.strawberry", "date_created": 1594457410.0, "description": "Use Strawberry as a better version", "distro": "Fedora", "karma_down": 5, "karma_up": 7, "locale": "de_AT", "rating": 20, "reported": 0, "review_id": 245655, "score": 0, "summary": "No Alsa output anymore", "user_display": "Harman Kardon", "version": "0.6.12-1.fc32"}, {"app_id": "org.strawberrymusicplayer.strawberry", "date_created": 1666966317.0, "description": "Excelente reproductor.", "distro": "Fedora Linux", "karma_down": 0, "karma_up": 0, "locale": "es_CO.UTF-8", "rating": 100, "reported": 0, "review_id": 533409, "score": 31, "summary": "Genial", "user_display": "Administrador", "version": "1.0.8"}, {"app_id": "org.strawberrymusicplayer.strawberry", "date_created": 1650849515.0, "description": "Es r\u00e1pida, clara, simple, intuitiva y tiene todo a mano. Era fan\u00e1tico de Jukebox y me encontr\u00e9 con Strawberry, enorme alegr\u00eda. Sin artificios innecesarios, con todo todo lo que se necesita en un reproductor!", "distro": "Debian GNU/Linux", "karma_down": 0, "karma_up": 0, "lo