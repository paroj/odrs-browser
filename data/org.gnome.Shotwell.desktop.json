[{"app_id": "org.gnome.Shotwell", "date_created": 1554193075.0, "description": "Only issue i have is that it doesn't support .heic formats (Apples unpopular image format). Please support this format soon. Otherwise it works perfectly fine with some simple editing tools.", "distro": "Fedora", "karma_down": 0, "karma_up": 0, "locale": "en_NZ", "rating": 80, "reported": 0, "review_id": 18530, "score": 0, "summary": "Great photo manager", "user_display": "Sergei", "version": "stable"}, {"app_id": "org.gnome.Shotwell", "date_created": 1573541010.0, "description": "The main key is NOTHING IS 100%. If you can handle that, than you will love the benefits Shotwell brings to a project. I never met an artist that only uses one brush, a professional photographer that only uses one camera, or an accomplished musician that only uses one instrument.", "distro": "Peppermint", "karma_down": 0, "karma_up": 0, "locale": "en_US", "rating": 100, "reported": 0, "review_id": 109407, "score": 0, "summary": "Excellent for Working with Photos shot in RAW", "user_display": "Fred Renee", "version": "0.30.5"}, {"app_id": "org.gnome.Shotwell", "date_created": 1585119695.0, "description": "Now it's only seeing my Picture and Download folders and nothing else anymore.", "distro": "Fedora", "karma_down": 0, "karma_up": 0, "locale": "en_US", "rating": 20, "reported": 0, "review_id": 170165, "score": 0, "summary": "used to be good", "user_display": "L", "version": "stable"}, {"app_id": "org.gnome.Shotwell", "date_created": 1586472936.0, "description": "I love Shotwell. It makes organizing my photos a breeze, and I love that it automatically imports my shots into folders organized by date.", "distro": "Fedora", "karma_down": 0, "karma_up": 0, "locale": "en_US.UTF-8", "rating": 100, "reported": 0, "review_id": 179856, "score": 0, "summary": "Great Photo Manager", "user_display": "Russel Taylor", "version": "stable"}, {"app_id": "org.gnome.Shotwell", "date_created": 1590702905.0, "description": "Easy to